              "enable_change_category":false,
              "guest_category_id":"",
              "document_name":"",
              "enable_icon":"",
              "button_label":"Add to my calendar",
              "step_number":"",
              "enable_custom_css":false,
              "button_color":"#000000",
              "button_text_color":"#FFFFFF"
            }
          },
          "bloc-1":{
            "type":"text_block",
            "settings":{
              "button_type":"registration_edit",
              "button_link":"",
              "website_page_url":"",
              "auto_submit":false,
              "enable_change_category":false,
              "guest_category_id":"",
              "document_name":"",
              "enable_icon":"",
              "button_label":"Edit my registration",
              "step_number":"",
              "enable_custom_css":false,
              "button_color":"#000000",
              "button_text_color":"#FFFFFF"
            }
          },
          "bloc-2":{
            "type":"text_block",
            "settings":{
              "button_type":"badge",
              "button_link":"",
              "website_page_url":"",
              "auto_submit":false,
              "enable_change_category":false,
              "guest_category_id":"",
              "document_name":"",
              "enable_icon":"",
              "button_label":"Download my badge",
              "step_number":"",
              "enable_custom_css":false,
              "button_color":"#000000",
              "button_text_color":"#FFFFFF"
            }
          }
        },
        "blocks_order":[
          "bloc-0",
          "bloc-1",
          "bloc-2"
        ]
      },
      "qrcode":{
        "type":"qrcode",
        "settings":{}
      },
      "footer":{
        "type":"footer",
        "settings":{
          "contact_email":"{{ event.reply_to_email }}",
          "contact_phone":"",
          "address":"{{ event.address }}",
          "copyright":"Â© {{ 'now' | date: '%Y' }} {{ event.organizer }} - All rights reserved",
          "color_bg_footer_1":"#f8f8f8",
          "color_text_footer_1":"{{ event.primary_color }}",
          "color_bg_footer_2":"{{ event.primary_color }}",
          "color_text_footer_2":"#FFFFFF"
        }
      }
    },
    "pages":{
      "index":[
        "image-with-overlay",
        "button-group"
      ],
      "info":[
        "map"
      ],
      "guests_list":[
        "guests-list"
      ],
